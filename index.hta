    applicationname="myapp"
    sysmenu="yes"
    caption="yes"
    singleinstance="yes"
    icon=""
   >
   <link rel="stylesheet" type="text/css" href="pico.min.css">
   <script>
      // alert("sysmenu    = " + ohta.sysmenu);
